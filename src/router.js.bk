import Vue from 'vue';
import Router from 'vue-router';
import PageA from './components/PageA.vue';
import PageB from './components/PageB.vue';

Vue.use(Router);

export default new Router({
    mode: 'history',
    routes: [
        {
            path: '/page-a',
            name: 'PageA',
            component: PageA,
        },
        {
            path: '/page-b',
            name: 'PageB',
            component: PageB,
        },
    ],
});
